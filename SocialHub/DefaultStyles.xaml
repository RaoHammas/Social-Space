<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <Color x:Key="ColorPrimary">#E1EAF7</Color>
    <Color x:Key="ColorWhite">White</Color>
    <Color x:Key="ColorSecondary">#007ACC</Color>

    <SolidColorBrush x:Key="ColorBrushPrimary" Color="{StaticResource ColorPrimary}" />
    <SolidColorBrush x:Key="ColorBrushWhite" Color="{StaticResource ColorWhite}" />
    <SolidColorBrush x:Key="ColorBrushSecondary" Color="#007ACC" />
    <!--<Style TargetType="TextBlock">
        <Setter Property="Foreground" Value="{StaticResource ColorSecondary}"></Setter>
    </Style>-->




</ResourceDictionary>